{
    "name": "mytryk/lumki",
    "description": "Laravel User Management for Laravel 8 with Jetstream. Forked from https://github.com/kineticamobile/lumki",
    "license": "MIT",
    "homepage": "https://github.com/mytryk/lumki",
    "keywords": ["Laravel", "Lumki", "Users Management", "Laravel 8"],
    "require": {
        "illuminate/support": "~8",
        "spatie/laravel-permission": "^5.0",
        "lab404/laravel-impersonate": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4|~5|~6",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Kineticamobile\\Lumki\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kineticamobile\\Lumki\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kineticamobile\\Lumki\\LumkiServiceProvider"
            ],
            "aliases": {
                "Lumki": "Kineticamobile\\Lumki\\Facades\\Lumki"
            }
        }
    },
    "scripts": {
        "test": "phpunit --testdox"
    }
}
